cartoon='Doremon is cute robot '
print(len(cartoon))
print(cartoon.upper())
print(cartoon.lower())
print(cartoon.casefold())
print(cartoon.encode())
print(cartoon.find('d'))
print(cartoon.find('m'))
print(cartoon.find('C'))
print(cartoon.find('c'))
print(cartoon.find('is'))
print(cartoon.find('cut'))
print(cartoon.replace('is','blue'))
print(cartoon.replace('robot','rakoon'))
print(cartoon.title())
